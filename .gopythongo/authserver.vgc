# the arguments below belong on the build server in envvars
git-include-commit-san

envdir=True
output=.gopythongo/etc/appconfig/authserver/env/
common-name=authserver
certfile-out=.gopythongo/etc/appconfig/authserver/files/build_cert.pem
keyfile-out=.gopythongo/etc/appconfig/authserver/files/build_key.pem
xsign-bundle-path=.gopythongo/etc/appconfig/authserver/files/
output-bundle-envvar=[VAULT_SSLCERT=vault.crt:/etc/appconfig/authserver/files,DB_SSLCERT=postgresql.crt:/etc/appconfig/authserver/files]
output-key-envvar=[VAULT_SSLKEY:/etc/appconfig/authserver/files,DB_SSLKEY:/etc/appconfig/authserver/files]
