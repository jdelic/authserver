-p authserver_{{debian_version.version}}.deb
-n authserver
--provides maurusnet-authserver
-v "{{debian_version.version}}"
-m "Jonas Maurus"
-d python3
-d python3-pip
-d python3-virtualenv
-d adduser
--before-remove .gopythongo/pre_rm.sh
--after-install .gopythongo/post_install.sh
--after-remove .gopythongo/post_rm.sh
--directories "{{basedir}}"
--directories /run/authserver
--deb-systemd .gopythongo/etc/systemd/authserver.service
--deb-systemd .gopythongo/etc/systemd/dkimsigner.service
{{basedir}}
.gopythongo/etc/appconfig/authserver/files/=/etc/appconfig/authserver/files/
